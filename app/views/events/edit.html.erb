<%= form_for @event do |f| %>

<%= f.label :title %>
<%= f.text_field :title, required:true %>

<%= f.label :description %>
<%= f.text_field :description, required:true %>

<%= f.label :date %>
<%= f.text_field :date, required:true %>

<%= f.label :verified, "Verify?" %>
<%= f.check_box :verified %>


<%= f.submit "Edit Event!", class:"button" %>
<%end%>
