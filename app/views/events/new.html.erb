<div class = "row">
<%= form_for @event do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title, required:true %>

  <%= f.label :description %>
  <%= f.text_field :description, required:true %>

  <%= f.label :date %>
  <%= f.text_field :date, required:true %>

  <%= f.label :event_image, "Select Your Image" %>
  <%= f.file_field :event_image, required:true %>

  <%= f.submit "Submit Event!", class:"button" %>



<% end %>
</div>
